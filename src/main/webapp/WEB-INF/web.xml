<?xml version="1.0" encoding="ISO-8859-1" ?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
         http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
         version="2.4">

    <welcome-file-list>
        <!--<welcome-file>index.html</welcome-file>-->
        <welcome-file>login.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>add</servlet-name>
        <servlet-class>ru.job4j.h1config.t2todolist.controller.AddServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>all</servlet-name>
        <servlet-class>ru.job4j.h1config.t2todolist.controller.LoadAllServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>not</servlet-name>
        <servlet-class>ru.job4j.h1config.t2todolist.controller.LoadNotPerformedServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>upload</servlet-name>
        <servlet-class>ru.job4j.h2mapping.t3carmarket.controller.FileUploadServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>addcar</servlet-name>
        <servlet-class>ru.job4j.h2mapping.t3carmarket.controller.AddCarServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>main</servlet-name>
        <servlet-class>ru.job4j.h2mapping.t3carmarket.controller.MainCarServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>fields</servlet-name>
        <servlet-class>ru.job4j.h2mapping.t3carmarket.controller.AllFieldsServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>log</servlet-name>
        <servlet-class>ru.job4j.h2mapping.t3carmarket.controller.LoginPassServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>day</servlet-name>
        <servlet-class>ru.job4j.h2mapping.t3carmarket.controller.DayServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>brand</servlet-name>
        <servlet-class>ru.job4j.h2mapping.t3carmarket.controller.BrandServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>add</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>all</servlet-name>
        <url-pattern>/loadAll</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>not</servlet-name>
        <url-pattern>/loadNot</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>addcar</servlet-name>
        <url-pattern>/addcar</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>main</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>fields</servlet-name>
        <url-pattern>/fields</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>log</servlet-name>
        <url-pattern>/log</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>day</servlet-name>
        <url-pattern>/day</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>brand</servlet-name>
        <url-pattern>/brand</url-pattern>
    </servlet-mapping>

</web-app>